{
  "name": "npm_package_starter",
  "version": "0.0.1",
  "description": "everything you need to start create your own npm package",
  "main": "./src/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faslin-kosta/hooks.git"
  },
  "keywords": [
    "hooks",
    "react"
  ],
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "author": "faslin_kosta",
  "contributors": [
    {
      "email": "vasil.kostadinov@3web-studio.com",
      "name": "Vasil Kostadinov",
      "url": "https://github.com/VasilKostadinov0104"
    },
    {
      "name": "Vasil Kostadinov",
      "url": "https://github.com/m258987"
    }
  ],
  "icon": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/faslin-kosta/hooks/issues"
  },
  "homepage": "https://github.com/faslin-kosta/hooks#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  }
}
